{
  "manifest_version": 3,
  "name": "Clear Cache",
  "version": "1.0",
  "description": "Clears cache of the current tab",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Clear Cache"
  },
  "permissions": [
    "browsingData"
  ]
}
